{
  "name": "BuildInfo",
  "description": "BuildInfo",
  "version": "0.0.4",
  "content_scripts": [
    {
      "matches": [
        "*://www.circleci.com/*",
        "*://circleci.com/*"
      ],
      "css": [
        "static/css/main.css"
      ],
      "js": [
        "inject.js",
        "static/js/main.js"
      ]
    }
  ],
  "permissions": [
    "cookies",
    "*://www.circleci.com/*",
    "*://circleci.com/*"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2
}
